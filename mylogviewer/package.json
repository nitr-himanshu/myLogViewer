{
  "name": "mylogviewer",
  "displayName": "myLogViewer",
  "description": "A custom log viewer which help in log analysis",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mylogviewer.highlightText",
        "title": "myLogViewer: Highlight Text"
      },
      {
        "command": "mylogviewer.clearHighlights",
        "title": "myLogViewer: Clear Highlights"
      },
      {
        "command": "mylogviewer.openSidebar",
        "title": "myLogViewer: Open Sidebar"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}